# Environment variables template
# Copy this to .env for local development

# Google OAuth Credentials - REPLACE WITH YOUR OWN VALUES
CLIENT_ID=your-google-oauth-client-id-here
CLIENT_SECRET=your-google-oauth-client-secret-here
REDIRECT_URI=http://localhost:3000/oauth2callback
REFRESH_TOKEN=your-google-oauth-refresh-token-here

# Application Authentication
AUTH_USERNAME=admin
AUTH_PASSWORD=secure123

# Flask Configuration
SECRET_KEY=your-secret-key-here
FLASK_ENV=production

# Server Configuration
PORT=5000